{
  "name": "diazoxide/wp-lib-option",
  "description": "WordPress Option",
  "keywords": [
    "wordpress",
    "admin",
    "interface",
    "ui",
    "dynamic",
    "form",
    "generator"
  ],
  "license": [
    "GPL-3.0-or-later"
  ],
  "minimum-stability": "dev",
  "prefer-stable": true,
  "require": {
    "php": ">=7.2",
    "ext-mbstring": "*",
    "ext-json": "*",
    "diazoxide/php-helpers": "dev-master",
    "yiisoft/html": "dev-master"
  },
  "require-dev": {
    "squizlabs/php_codesniffer": "*"
  },
  "autoload": {
    "psr-4": {
      "diazoxide\\wp\\lib\\option\\": "src/"
    }
  },
  "scripts": {
    "style": "phpcs --standard=PSR12"
  },
  "repositories": [
    {
      "type": "composer",
      "url": "https://asset-packagist.org"
    }
  ]
}
